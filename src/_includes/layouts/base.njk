<!doctype html>
<html lang="en">
  {% block css %}{% endblock %}
  {% include "blocks/head.njk" %}
  <body>
    {% include "googletagmanager-noscript.njk" %}
    {% if nav %}
      {% include "blocks/nav.njk" %}
    {% endif %}
    {{ layoutContent | safe }}
    {% block content %}{% endblock %}

    {% include "blocks/newsletter.njk" %}
    {% include "blocks/footer.njk" %}
    <script src="/_assets/js/script.js?v={{ data.buildRef }}"></script>
    {% block scripts %}{% endblock %}
    {% if aos != false %}
      <script>AOS.init();</script>
    {% endif %}
  </body>
</html>
